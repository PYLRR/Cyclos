// example of comment
/* example of comment
on several
lines */

public:
a : typea
b:typeb

private:
n1:noncem
n2:noncem
ska:kaenca
skb:kaencb
skc:kaencc
k0:noncek

%emission cles pub
out( 0 , < pub ( ska ) , pub (  skb ) > )

%role a avec b par a
out(1 ,aenc(sign( < a, b,k0>, ska),pub(skb)))
in(1, senc(xa0: noncem,k0) )

%role a avec b par b
in(2,aenc(sign(<a,b,xb :noncek>,ska),pub(skb)))
out(2,senc (n1 ,xb))



